<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.InsertMyStudentController">
   <children>
      <VBox spacing="5.0">
         <children>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Label text="이름">
                     <font>
                        <Font name="D2Coding" size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfName" prefWidth="100.0" promptText="이름" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Label text="국어">
                     <font>
                        <Font name="D2Coding" size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfKor" prefWidth="100.0" promptText="국어점수" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Label text="수학">
                     <font>
                        <Font name="D2Coding" size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfMath" prefWidth="100.0" promptText="수학점수" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Label text="영어">
                     <font>
                        <Font name="D2Coding" size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfEng" prefWidth="100.0" promptText="영어점수" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Button fx:id="btnInsert" mnemonicParsing="false" onAction="#insertAction" text="저장" />
                  <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#cancelAction" text="취소" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="13.0" left="13.0" right="13.0" top="13.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
